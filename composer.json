{
    "name": "smodav/auth",
    "description": "Auth boilerplate with 2fa for laravel",
    "type": "library",
    "keywords": ["cms", "packagers", "laravel", "boilerplate", "2fa"],
    "license": "MIT",
    "authors": [
        {
            "name": "David Mjomba",
            "email": "smodavprivate@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/Auth/Support/helpers.php",
            "src/Auth/Support/constants.php"
        ],
        "psr-4": {
            "SmoDav\\": "src/"
        }
    },
     "autoload-dev": {
        "psr-4": {
            "SmoDav\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "bacon/bacon-qr-code": "~1.0",
        "pragmarx/google2fa": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "~5.7"
    },
    "minimum-stability": "stable"
}
